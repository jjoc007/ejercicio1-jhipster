<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Category.
    -->
    <changeSet id="20200310045100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="category_id"
                                 baseTableName="category_id"
                                 constraintName="fk_category_id_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="category"/>

        <addForeignKeyConstraint baseColumnNames="id_id"
                                 baseTableName="category_id"
                                 constraintName="fk_category_id_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transmition"/>
        
    </changeSet>
</databaseChangeLog>
